/* button start*/
.em-btn.data-v-5350d404 {
  width: 100%;
  position: relative;
  border: 0 !important;
  border-radius: 10rpx;
  display: inline-block;
}
.em-btn.data-v-5350d404::after {
  content: "";
  position: absolute;
  width: 200%;
  height: 200%;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scale(0.5, 0.5);
  transform: scale(0.5, 0.5);
  box-sizing: border-box;
  left: 0;
  top: 0;
  border-radius: 20rpx;
}
.em-btn-block.data-v-5350d404 {
  font-size: 36rpx;
  height: 90rpx;
  line-height: 90rpx;
}
.em-white.data-v-5350d404::after {
  border: 1px solid #eaeef1;
}
.em-white-hover.data-v-5350d404 {
  background: #e5e5e5 !important;
  color: #2e2e2e !important;
}
.em-dark-disabled.data-v-5350d404 {
  opacity: 0.6 !important;
  color: #fafbfc !important;
}
.em-outline-hover.data-v-5350d404 {
  opacity: 0.5;
}
.em-primary-hover.data-v-5350d404 {
  background: #4a67d6 !important;
  color: #e5e5e5 !important;
}
.em-primary-outline.data-v-5350d404::after {
  border: 1px solid #5677fc !important;
}
.em-primary-outline.data-v-5350d404 {
  color: #5677fc !important;
  background: none;
}
.em-danger-hover.data-v-5350d404 {
  background: #d53912 !important;
  color: #e5e5e5 !important;
}
.em-danger-outline.data-v-5350d404 {
  color: #fc4d52 !important;
  background: none;
}
.em-danger-outline.data-v-5350d404::after {
  border: 1px solid #fc4d52 !important;
}
.em-red-hover.data-v-5350d404 {
  background: #c51a15 !important;
  color: #e5e5e5 !important;
}
.em-red-outline.data-v-5350d404 {
  color: #e41f19 !important;
